<div class='container bookings-container'>
  <div class='bookings-container'>
    <div class='booking-list'>
      <h2>Party Type</h2> 
      <h2>Animal</h2>
      <h2>Hours</h2>
      <h2>Address</h2>
      <h2>Total Price</h2>
    </div>
    <% @bookings.each do |booking| %>
      <div class='booking-list'>
        <h5><%= booking.party_type %></h5>
        <h5><%= link_to booking.party_animal.name, party_animal_path(booking.party_animal) %></h5>
        <h5><%= ((booking.end_time - booking.start_time) / 3600).to_i %></h5>
        <h5><%= booking.address %></h5>
        <h5><%= booking.total_price %>â‚¬</h5>
        <%= link_to booking_path(booking), method: :delete, data: { confirm: "Delete this booking from your dashboard?" } do %>
        <i class="far fa-trash-alt"></i>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
